{
	"name": "vscode-taskkill",
	"displayName": "TaskKill",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.showActiveNetworkTasks",
		"onCommand:extension.forceKillActiveNetworkTasks",
		"onCommand:extension.killActiveNetworkTask"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
				"command": "extension.showActiveNetworkTasks",
				"title": "Show active network tasks"
			},
			{
				"command": "extension.forceKillActiveNetworkTasks",
				"title": "Kill all active network tasks"
			},
			{
				"command": "extension.killActiveNetworkTask",
				"title": "Kill an active network task (by port number)"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/getos": "^3.0.0",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"@types/shelljs": "^0.8.5",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"getos": "^3.1.1",
		"shelljs": "^0.8.3",
		"table-parser": "^1.0.1"
	}
}